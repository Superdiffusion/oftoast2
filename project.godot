; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "tvn",
"language": "GDScript",
"path": "res://scripts/tvn.gd"
} ]
_global_script_class_icons={
"tvn": ""
}

[application]

config/name="oftoast2"
config/description="THAT'S RIGHT OFTOAST II 2 BABY"
run/main_scene="res://scenes/loader.tscn"
config/custom_user_dir_name="toastware"
run/low_processor_mode=true
boot_splash/image="res://unknown.png"
config/icon="res://assets/toast.png"

[display]

window/size/width=1440
window/size/height=1080
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

back=false
resize=false

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=2
environment/default_clear_color=Color( 0.345098, 0.254902, 0.411765, 1 )
quality/filters/use_fxaa=true
environment/default_environment="res://default_env.tres"
